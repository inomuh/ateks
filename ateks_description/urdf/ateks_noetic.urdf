<?xml version='1.0'?>
<robot name='ateks'>
  
  <!-- Base Footprint -->
    <link name="base_footprint"/> 
    <joint name="base_footprint_joint" type="fixed">     	
	<parent link="base_footprint"/>
     	<child link="base_link"/>	
    </joint> 
    
    
   <!-- Base_Link -->
   
    <link name='base_link'>
    	<inertial>
      	  <mass value="83.7"/>
      	  <origin xyz="0 0 0"/>
      	  <inertia ixx="10.0" ixy="10.0" ixz="10.0" iyy="10.0" iyz="10.0" izz="100.0" />
    	</inertial>
    
    	<visual>
      	  <origin xyz="0.61 0.33 0.1" rpy="0 0.4 3.14"/>
      	    <geometry>
        	<!--<box size ="0.33 0.42 0.05"/>-->
        	    <mesh filename="package://ateks_description/meshes/ateks2.stl" scale="0.001 0.001 0.001"/>
      	    </geometry>
        <!-- <origin xyz="0 0 0.2" rpy="0 0 0" /> -->
        <!-- <geometry> -->
	<!--   <box size="1.1 0.5 0.1"/> -->
        <!-- </geometry> -->
    	</visual>
    	
    	<collision>
      	   <origin rpy="0 0 0" xyz="0 0 0.2"/>
      	    <geometry>
        	    <box size ="1.08 0.6 0.1"/>
      	    </geometry>
    	</collision> 
    	  
     </link>
     
     <gazebo reference="base_link">
    	<material value="Gazebo/DarkGrey "/>
     </gazebo>
     
  <!-- Caster Wheels -->
    <link name='left_caster_wheel'>
    	<collision>
      	   <origin rpy="0 0 0" xyz="0.33884 -0.28539 0.075"/>
      	    <geometry>
      	        <sphere radius ="0.075"/>
      	    </geometry>
    	</collision>   
     </link>
     
     <joint name="left_caster_joint" type="fixed">
	      <origin xyz="0 0 0" rpy="0 0 0"/>
	      <parent link="base_link"/>
	      <child link="left_caster_wheel"/>
	      <axis xyz="0 1 0"/>
     </joint> 

    <link name='right_caster_wheel'>
    	<collision>
      	   <origin rpy="0 0 0" xyz="0.33884 0.28539 0.075"/>
      	    <geometry>
      	        <sphere radius ="0.075"/>
      	    </geometry>
    	</collision>   
     </link>
     
     <joint name="right_caster_joint" type="fixed">
	      <origin xyz="0 0 0" rpy="0 0 0"/>
	      <parent link="base_link"/>
	      <child link="right_caster_wheel"/>
	      <axis xyz="0 1 0"/>
     </joint>   
     
    <gazebo reference="left_caster_wheel">
      <mu1>0.2</mu1>
      <mu2>0.2</mu2>
    </gazebo>
    <gazebo reference="right_caster_wheel">
      <mu1>0.2</mu1>
      <mu2>0.2</mu2>
    </gazebo>
    
     
  <!-- Back Wheels -->
  
    <link name='right_wheel'>
    	<inertial>
      	  <mass value="1.0"/>
      	  <origin xyz="0.0 0.0 0.0"/>
	  <inertia ixx="1.0" ixy="0" ixz="0" iyy="1.0" iyz="0" izz="1.0" />
    	</inertial>
    
    	<collision>
      	   <origin rpy="1.57 0 0" xyz="0.0 0.0 0.0"/>
      	    <geometry>
      	        <cylinder radius="0.1925" length="0.05"/>
      	    </geometry>
    	</collision>   
     </link>
     
     <joint name="right_wheel_joint" type="continuous">
    	<axis xyz="0 1 0"/>
    	<origin xyz="-0.31 0.325 0.1925" rpy="0 0 0"/>
    	<parent link="base_link"/>
    	<child link="right_wheel"/>
	<dynamics damping="0.7"/>
     </joint>
     
    <link name='left_wheel'>
    	<inertial>
      	  <mass value="1.0"/>
      	  <origin xyz="0.0 0.0 0.0"/>
	  <inertia ixx="1.0" ixy="0" ixz="0" iyy="1.0" iyz="0" izz="1.0" />
    	</inertial>
    
    	<collision>
      	   <origin rpy="1.57 0 0" xyz="0.0 0.0 0.0"/>
      	    <geometry>
      	        <cylinder radius="0.1925" length="0.05"/>
      	    </geometry>
    	</collision>   
     </link>
     
     <joint name="left_wheel_joint" type="continuous">
    	<axis xyz="0 1 0"/>
    	<origin xyz="-0.31 -0.325 0.1925" rpy="0 0 0"/>
    	<parent link="base_link"/>
    	<child link="left_wheel"/>
	<dynamics damping="0.7"/>
     </joint>
     


<!-- Camera Port -->
    <link name="kinect_link">
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <mass value="1.0" />
        <origin xyz="0 0 0" />
        <inertia ixx="1.0" ixy="0" ixz="0" iyy="1.0" iyz="0" izz="1.0" />
      </inertial>

      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
        <mesh filename="package://ateks_description/meshes/kinect.dae" scale="1 1 1"/>
      	  <!-- <sphere radius=".1"/> -->
        </geometry>
		</visual>
	</link>

    <link name="kinect_dummy">
       <pose>0 0 0 0 0 0</pose>
    </link>
	
    <joint name="kinect_joint_dummy" type="fixed">
      <origin xyz="0 0 0" rpy="-1.57 0 -1.57"/>
      <parent link="kinect_link"/>
      <child link="kinect_dummy"/>
      <axis xyz="0 1 0"/>
    </joint>	
    
    <link name="kinect_dummy2">
      <pose>0 0 0 0 0 0</pose>
	</link>
	
    <joint name="kinect_joint_dummy2" type="fixed">
      <origin xyz="0 0 0" rpy="-1.57 0 -1.57"/>
      <parent link="kinect_link2"/>
      <child link="kinect_dummy2"/>
      <axis xyz="0 1 0"/>
    </joint>	
		
    <link name="kinect_link2">
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <mass value="1.0" />
        <origin xyz="0 0 0" />
        <inertia ixx="1.0" ixy="0" ixz="0" iyy="1.0" iyz="0" izz="1.0" />
      </inertial>

      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
        <mesh filename="package://ateks_description/meshes/kinect.dae" scale="1 1 1"/>
      	  <!-- <sphere radius=".1"/> -->
        </geometry>
	</visual>

      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
      	  <sphere radius=".1"/>
        </geometry>
      </collision>
    </link>

    <joint name="kinect_joint" type="continuous">
      <origin xyz="-0.45 -0.2 1.54" rpy="0 0.38 -0.349"/> <!--0.38 -0.349-->
      <parent link="base_link"/>
      <child link="kinect_link"/>
      <axis xyz="0 1 0"/>
    </joint>

    <joint name="kinect_joint2" type="fixed">
      <origin xyz="-0.45 -0.2 1.68" rpy="0 0.38 0.698"/>  <!--0 0.38 0.698-->
      <parent link="base_link"/>
      <child link="kinect_link2"/>
      <axis xyz="0 1 0"/>
    </joint>
    
